th; r++) {\n          var obj = {};\n          obj[\"Word\"] = this.words.data[[0]][r];\n          obj[\"Points\"] = this.words.data[[1]][r];\n          this.items[[this.words.data[[0]][r].length - 2]].push(obj);\n          if (!this.sizes.includes(this.words.data[[0]][r].length - 2)) {\n            this.sizes.push(this.words.data[[0]][r].length - 2);\n          }\n        }\n      }\n      this.sizes.sort().reverse();\n    },\n    letters() {\n      this.letters = this.letters.toUpperCase();\n    }\n  },\n  computed: {\n    checkChars() {\n      var wilds = 0;\n      var output = [];\n      var input = this.letters;\n      for (var y = 0; y < input.length; y++) {\n        for (var i = 0; i < 27; i++) {\n          if (input[y] == this.alf[i] && i == 26 && wilds < 2) {\n            wilds++;\n          } else if (input[y] == this.alf[i] && i < 26) {\n            output.push(input[y]);\n            i++;\n          }\n        }\n      }\n      output = output.slice(0, 14 - wilds);\n      output.push(wilds.toString());\n      return output;\n    }\n  },\n  methods: {\n    async findWords() {\n      if (this.searched) {\n        this.sizes.splice(0);\n        this.words.data.splice(0);\n        for (var c = 0; c < 12; c++) {\n          this.items[[c]].splice(0);\n        }\n      }\n      this.searched = false;\n      this.words = await WordService.findWords(this.checkChars);\n      this.searched = true;\n    }\n  },\n  async created() {\n    try {\n      this.words = await WordService.findWords();\n    } catch (err) {\n      this.error = err.message;\n    }\n  }\n};\n</script>\n<!--=================================================================================-->\n<!--=====================================================================STYLING=====-->\n<!--=================================================================================-->\n<style scoped>\n.word-container {\n  width: 100vw;\n  margin: 0;\n  position: relative;\n  left: 0;\n  padding: 0;\n  top: 80px;\n