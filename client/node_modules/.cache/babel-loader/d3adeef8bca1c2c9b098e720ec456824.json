his.fromTop * -0.36;\n      this.forestT = this.fromTop * -0.29;\n      this.starsT = this.fromTop * -0.8;\n\n    }\n  }\n  \n}\n</script>\n<!--=================================================================================-->\n<!--=====================================================================STYLING=====-->\n<!--=================================================================================-->\n<style>\n.first-look{\n  position: relative;\n  height: 100vh;\n  width: 100%;\n  margin: 0;\n  padding: 0;\n  left: 0;\n  top: 0;\n}\n/*==============================FOREST-IMAGE & TAG=====*/\n.firstlook,\n.tagline {\n\tposition: fixed;\n  z-index: -3;\n  width: 100vw;\n  height: 100vh;\n  margin: 0;\n  padding: 0;\n}\n\n.tagline {\n\tleft: 0px;\n}\n\n#forMe {\n\tz-index: -4;\n  position: absolute;\n  top: 0;\n  padding: 0;\n\tmargin: 0;\n}\n\n#kevinCox,\n#webDeveloper {\n\tposition: absolute;\n}\n\n#kevinCox {\n\topacity: 0.75;\n}\n\n#webDeveloper {\n\topacity: 0.85;\n}\n\n/*======================================STAR-IMAGE=====*/\n.starBg {\n\tposition: fixed;\n\twidth: 100vw;\n\theight: 100vh;\n}\n\n#purpStarz {\n\tposition: absolute;\n\tobject-fit: fill;\n\ttop: calc(100vh + 15vw + 10px);\n\twidth: 100%;\n\theight: auto;\n\tmargin: 0;\n}\n/*======================================SCROLLING====*/\n.bubbles {\n\tposition: fixed;\n\tdisplay: block;\n\tz-index: 30;\n\theight: 60vh;\n\twidth: 20px;\n\ttop: 26vh;\n\tleft: 93vw;\n}\n\n.bubble {\n\tposition: absolute;\n\tleft: 0;\n\theight: 20px;\n\twidth: 20px;\n\tbackground: #BCBCBC99;\n\tborder-radius: 10px;\n\tbox-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.4);\n\tborder: 3.5px solid white;\n}\n\n.b1 {\n\ttop: 0;\n}\n\n.b2 {\n\ttop: 20%;\n}\n\n.b3 {\n\ttop: 40%;\n}\n\n.b4 {\n\ttop: 60%;\n}\n\n.b5 {\n\ttop: 80%;\n}\n\n.scrlArrowz {\n\tposition: absolute;\n\theight: 100px;\n\twidth: 60px;\n\tleft: calc(50% - 30px);\n\tbottom: 25px;\n\tz-index: 40;\n}\n\n.scrlAr {\n\tposition: absolute;\n\twidth: 60px;\n\theight: 60px;\n\topacity: 0;\n\tobject-fit: contain;\n\t-webkit-animation: mymove1 3s ease infinite;\n\tanimation: mymove1 3s ease infinite;\n}\n\n.scrollWord {\n\tposition: absolute;\n\theight: 33px;\n\twidth: 100%;\n\tleft: 0;\n\tz-index: 10;\n\tbottom: 0;\n}\n\n.scrolley {\n\topacity: 0.5;\n\t-webkit-animation: mymove2 1s ease infinite;\n\tanimation: mymove2 1s ease infinite;\n\t-webkit-animation-delay: 1s;\n\tanimation-delay: 1s;\n}\n\n#scrD2 {\n\t-webkit-animation-delay: 1.5s;\n\tanimation-delay: 1.5s;\n}\n\n#scrD3 {\n\t-webkit-animation-delay: 2.5s;\n\tanimation-delay: 2.5s;\n}\n\n@keyframes mymove1 {\n\t0% {\n\t\ttop: -30px;\n\t\tleft: 23px;\n\t\twidth: 14px;\n\t\topacity: 0.0;\n\t}\n\n\t33% {\n\t\ttop: 23px;\n\t\tleft: 5px;\n\t\twidth: 50px;\n\t\topacity: 0.45;\n\t}\n\n\t66% {\n\t\ttop: 40px;\n\t\tleft: 5px;\n\t\twidth: 50px;\n\t\topacity: 0.45;\n\t}\n\n\t100% {\n\t\ttop: 65px;\n\t\tleft: 23px;\n\t\twidth: 14px;\n\t\topacity: 0.0;\n\t}\n}\n\n@-webkit-@keyframes mymove1 {\n\t0% {\n\t\ttop: -30px;\n\t\tleft: 23px;\n\t\twidth: 14px;\n\t\topacity: 0.0;\n\t}\n\n\t33% {\n\t\ttop: 23px;\n\t\tleft: 5px;\n\t\twidth: 50px;\n\t\topacity: 0.45;\n\t}\n\n\t66% {\n\t\ttop: 40px;\n\t\tleft: 5px;\n\t\twidth: 50px;\n\t\topacity: 0.45;\n\t}\n\n\t100% {\n\t\ttop: 65px;\n\t\tleft: 23px;\n\t\twidth: 14px;\n\t\topacity: 0.0;\n\t}\n}\n\n@keyframes mymove2 {\n\t0% {\n\t\topacity: 0.3;\n\t}\n\n\t50% {\n\t\topacity: 0.7;\n\t}\n\n\t100% {\n\t\topacity: 0.3;\n\t}\n}\n\n@-webkit-@keyframes mymove2 {\n\t0% {\n\t\topacity: 0.3;\n\t}\n\n\t50% {\n\t\topacity: 0.7;\n\t}\n\n\t100% {\n\t\topacity: 0.3;\n\t}\n}\n\n#top {\n\tposition: absolute;\n\ttop: 0;\n}\n\n\n\n</style>\n\n"],"sourceRoot":"src/components/HomeComponents"}]}